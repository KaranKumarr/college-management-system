<body>


    <%- include("./partials/InstructorNavbar.ejs") %>

        <section class="Attendance">

            <h1 class="Attendance-Heading-Box">Attendance Dated: <%=date%>
            </h1>
            <ul>
                <h1 class="Attendance-Heading-Box"></h1>
                <%Attendance.forEach((Attend,i)=>{%>
                    <li class="Attendance-Box" style="justify-content: space-between;">

                        <h5>
                            <%=Attend.StudentName%>
                        </h5>
                        <h5>
                            <%=Attend.StudentID%>
                        </h5>
                        <h5>
                            <%=Attend.wasPresent%>
                        </h5>
                    </li>
                    <%})%>

            </ul>


        </section>

</body>

