<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>

    <link rel="stylesheet" href="/assets/index.css">
    <link rel="stylesheet" href="/assets/Navbar.css">
    <link rel="stylesheet" href="/assets/ExamsSchedule.css">
</head>

<body>

    <%- include("./partials/StudentNavbar.ejs") %>

        <section class="Exams">
            <span class="Exams-Heading-Box">
                <h1>DATE</h1>
                <h1>STATUS</h1>
            </span>

            <ul class="Exams-Data">
                <%ExamSchedule.forEach((schedule)=>{%>

                    <li class="Exams-Box">
                        <h5>
                            <%=schedule.CourseName%>
                        </h5>
                        <h5>
                            <%=schedule.ExamDate%>
                        </h5>
                    </li>

                    <%})%>
            </ul>
        </section>


</body>

</html>